; OOD

(declare-const X_0 Real)
(declare-const X_1 Real)

(declare-const Y_0 Real)
(declare-const Y_1 Real)
(declare-const Y_2 Real)

; Input constraints:
(assert (<= X_0 0.0))
(assert (>= X_0 -2.0))

(assert (<= X_1 0.0))
(assert (>= X_1 -2.0))

; Output constraints:
(assert (or
     (and (>= Y_0 Y_2))
     (and (>= Y_1 Y_2))
))
