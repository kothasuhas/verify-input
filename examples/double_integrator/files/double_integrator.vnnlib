; DoubleIntegrator

(declare-const X_0 Real)
(declare-const X_1 Real)

(declare-const Y_0 Real)
(declare-const Y_1 Real)

; Input constraints:
(assert (<= X_0 5.0))
(assert (>= X_0 -5.0))

(assert (<= X_1 5.0))
(assert (>= X_1 -5.0))

; Output constraints:
(assert (<= Y_0 5.0))
(assert (>= Y_0 4.5))
(assert (<= Y_1 0.25))
(assert (>= Y_1 -0.25))